<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal">
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:rowCount="3"
        android:columnCount="9"
        android:background="@color/black">

        <TextView
            android:id="@+id/text_id"
            android:layout_column="0"
            android:layout_row="0"
            android:layout_rowSpan="2"
            android:gravity="center"
            android:textSize="@dimen/list_smallsmalltext"
            android:textColor="@color/neitral_color"
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"/>
        <TextView
            android:id="@+id/text_date"
            android:layout_column="1"
            android:layout_row="0"
            android:gravity="center"
            android:textSize="@dimen/list_smalltext"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:maxLength="8" />
        <TextView
            android:id="@+id/text_time"
            android:layout_column="1"
            android:layout_row="1"
            android:gravity="center"
            android:textSize="@dimen/list_smallsmalltext"
            android:textColor="@color/neitral_color"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:maxLength="8" />
        <TextView
            android:id="@+id/text_move"
            android:layout_column="2"
            android:layout_row="0"
            android:layout_rowSpan="2"
            android:gravity="center"
            android:textSize="@dimen/list_text"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"/>
        <TextView
            android:id="@+id/text_moveseq"
            android:layout_column="3"
            android:layout_row="0"
            android:layout_rowSpan="2"
            android:textSize="@dimen/list_smalltext"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textAlignment="center"/>
        <TextView
            android:id="@+id/text_good"
            android:layout_column="4"
            android:layout_row="0"
            android:layout_rowSpan="2"
            android:gravity="center"
            android:textSize="@dimen/list_text"
            android:textColor="@color/good_color_green"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_bad"
            android:layout_column="5"
            android:layout_row="0"
            android:layout_columnSpan="3"
            android:gravity="center"
            android:textSize="@dimen/list_text"
            android:textColor="@color/bad_color"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_eval"
            android:layout_column="5"
            android:layout_row="1"
            android:gravity="center"
            android:textSize="@dimen/list_smalltext"
            android:textColor="@color/white"
            android:layout_width="20dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/StaticText"
            android:layout_column="6"
            android:layout_row="1"
            android:gravity="center"
            android:text=" / "
            android:textSize="@dimen/list_smalltext"
            android:textColor="@color/neitral_color"
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            tools:ignore="HardcodedText"/>
        <TextView
            android:id="@+id/text_evalmax"
            android:layout_column="7"
            android:layout_row="1"
            android:gravity="center"
            android:textSize="@dimen/list_smalltext"
            android:textColor="@color/white"
            android:layout_width="20dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_hint"
            android:layout_column="8"
            android:layout_row="0"
            android:layout_rowSpan="2"
            android:gravity="center"
            android:textSize="@dimen/list_text"
            android:textColor="@color/Evals"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_note"
            android:layout_column="0"
            android:layout_row="2"
            android:layout_columnSpan="9"
            android:textSize="@dimen/list_smalltext"
            android:textColor="@color/white"
            android:layout_width="405dp"
            android:textStyle="bold"
            android:layout_height="0dp"/>
    </GridLayout>
</LinearLayout>